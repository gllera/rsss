{
    "name": "rsss",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "start": "node --http-parser=legacy ./src/bin/www",
        "dev": "concurrently -p '{name}' -n 'RSSS,GULP' -c 'bgRed,bgBlue' -k 'npm run gql' 'npm run gserve'",
        "gql": "cross-env DEBUG=rsss:* nodemon --use_strict --http-parser=legacy ./src/bin/www.js",
        "gserve": "cross-env BASE_URL='http://localhost:3000' gulp serve",
        "build": "gulp build",
        "clean": "gulp clean"
    },
    "nodemonConfig": {
        "ext": "js,sql,graphql,yaml",
        "ignore": [
            "dist/*",
            "src_ui/*",
            "gulpfile.js"
        ]
    },
    "dependencies": {
        "async": "^3.2.0",
        "axios": "^0.21.1",
        "cheerio": "^1.0.0-rc.5",
        "cors": "^2.8.5",
        "debug": "~4.3.1",
        "express": "~4.17.1",
        "express-fileupload": "^1.2.1",
        "express-graphql": "^0.12.0",
        "fast-xml-parser": "^3.18.0",
        "graphql": "^15.5.0",
        "graphql-fields": "^2.0.3",
        "graphql-tools": "^7.0.2",
        "he": "^1.2.0",
        "html-minifier": "^4.0.0",
        "iconv-lite": "^0.6.2",
        "js-yaml": "^4.0.0",
        "loadsh": "0.0.4",
        "morgan": "~1.10.0",
        "sanitize-html": "^2.3.2",
        "sqlite": "^4.0.19",
        "sqlite3": "^5.0.1"
    },
    "devDependencies": {
        "bootstrap": "^4.6.0",
        "browser-sync": "^2.26.14",
        "browserify": "^17.0.0",
        "cash-dom": "^8.1.0",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.3",
        "graphql.js": "^0.6.7",
        "gulp": "^4.0.2",
        "gulp-clean": "^0.4.0",
        "gulp-pug": "^4.0.1",
        "gulp-replace": "^1.0.0",
        "gulp-sass": "^4.1.0",
        "gulp-sourcemaps": "^3.0.0",
        "hotkeys-js": "^3.8.2",
        "nodemon": "^2.0.7",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0"
    }
}
