{
    "name": "rsss",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node --http-parser=legacy ./src/bin/www",
        "dev": "concurrently -p '{name}' -n 'RSSS,GULP' -c 'bgRed,bgBlue' -k 'npm run gql' 'npm run gserve'",
        "gql": "cross-env DEBUG=rsss:* nodemon --use_strict --http-parser=legacy ./src/bin/www",
        "gserve": "cross-env BASE_URL='http://localhost:3000' gulp serve",
        "build": "gulp build",
        "clean": "gulp clean"
    },
    "nodemonConfig": {
        "ext": "js,sql,graphql,yaml",
        "ignore": [
            "dist/*",
            "src_ui/*",
            "gulpfile.js"
        ]
    },
    "dependencies": {
        "async": "^3.2.0",
        "async-mutex": "^0.2.4",
        "cheerio": "^1.0.0-rc.3",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "express-fileupload": "^1.1.9",
        "express-graphql": "^0.9.0",
        "fast-xml-parser": "^3.17.4",
        "graphql": "^14.7.0",
        "graphql-import": "^0.7.1",
        "graphql-tools": "^4.0.8",
        "he": "^1.2.0",
        "html-minifier": "^4.0.0",
        "iconv-lite": "^0.5.2",
        "js-yaml": "^3.14.0",
        "loadsh": "0.0.4",
        "morgan": "~1.9.1",
        "request": "^2.88.2",
        "sanitize-html": "^1.27.2",
        "sqlite": "^4.0.12",
        "sqlite3": "^4.2.0"
    },
    "devDependencies": {
        "bootstrap": "^4.5.0",
        "browser-sync": "^2.26.12",
        "browserify": "^16.5.1",
        "cash-dom": "^4.1.5",
        "concurrently": "^5.2.0",
        "cross-env": "^6.0.3",
        "graphql.js": "^0.6.6",
        "gulp": "^4.0.2",
        "gulp-clean": "^0.4.0",
        "gulp-pug": "^4.0.1",
        "gulp-replace": "^1.0.0",
        "gulp-sass": "^4.1.0",
        "gulp-sourcemaps": "^2.6.5",
        "hotkeys-js": "^3.8.1",
        "node-sass": "^4.14.1",
        "nodemon": "^2.0.4",
        "sass": "^1.26.10",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0"
    }
}
